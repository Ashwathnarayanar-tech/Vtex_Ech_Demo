{
  "footer": {
    "blocks": ["footer-layout.desktop", "footer-layout.mobile"]
  },
  "footer-layout.desktop": {
    "children": [
      "newsletter",
      "app-footer",
      "flex-layout.row#footer-1-desktop",
      "flex-layout.row#social-icons",
      "flex-layout.row#footer-3-desktop",
      "flex-layout.row#footer-4-desktop",
      "flex-layout.row#footer-5-desktop"
    ]
  },
  "flex-layout.row#newsletter":{
    "children":[ "newsletter"],
    "props": {
      "blockClass": "footer-link"
    }
  },
  "flex-layout.row#footer-1-desktop": {
    "children": [
      "vtex.menu@2.x:menu#footer-products",
      "vtex.menu@2.x:menu#footer-buying",
      "vtex.menu@2.x:menu#footer-for-owners",
      "vtex.menu@2.x:menu#footer-legal",
      "flex-layout.col#social-network",
      "vtex.menu@2.x:menu#buying",
      "vtex.menu@2.x:menu#footer-forOwners",
      "vtex.menu@2.x:menu#footer-legal"
    ],
    "props": {
      "blockClass": "footer-link",
      "paddingTop": 6,
      "paddingBottom": 3
    }
  },
  "flex-layout.col#social-network": {
    "children":[
      "social-networks",
      "rich-text#social"
    ],
    "props": {
      "blockClass": "social-network-col"
    }
  },
  "rich-text#social": {
    "props": {
      "text": "All prices are Manufacturers Suggested Retail Prices and do not include [tax and] installation charges. Dealer sets actual price.",
      "blockClass": "social"
    }
  },
  "newsletter": {
    "props": {
      "label": "Newsletter Subscription \n Nissan OEM Parts",
      "placeholder": "Enter Your Email Address",
      "blockClass": "newsletter"
    }
  },
  "social-networks": {
    "props": {
      "showInColor": false,
      "socialNetworks": [
        {
          "name": "Facebook",
          "url": "https://www.facebook.com/Echidna/"
        },
        {
          "name": "Instagram",
          "url": "https://www.instagram.com/echidna/"
        },
        {
          "name": "youtube",
          "url": "https://www.youtube.com/channel/UCRb6sTiZi34CD7-kolwZMow"
        },
        {
          "name": "twitter",
          "url": "https://twitter.com/echidnainc"
        },
        {
          "name": "Linkedin",
          "url": "https://www.linkedin.com/company/echidna"
        }
      ],
  "flex-layout.row#social-icons": {
    "children": ["image#test1","image#test2","image#test3","image#test4","image#test5"],
 "props":{
    "preventHorizontalStretch":true,
    "blockClass":"socialIcons"
 }
},

"image#test1":{
    "props":{
        "src":"https://parts.nissanusa.com/images/nissan-oem-assets/icons/social-faceboook-white.png",
        "maxHeight":45,
        "blockClass":"fb",
        "link": {
            "attributeNofollow": false,
            "attributeTitle": "",
            "newTab": true,
            "url": "/"
          }
    }
},
"image#test2":{
    "props":{
        "src":"https://parts.nissanusa.com/images/nissan-oem-assets/icons/social-youtube-white.png",
        "maxHeight":45,
        "blockClass":"youtube",
        "link": {
            "attributeNofollow": false,
            "attributeTitle": "",
            "newTab": true,
            "url": "/"
          }
    }
},
"image#test3":{
    "props":{
        "src":"https://parts.nissanusa.com/images/nissan-oem-assets/icons/social-twitter-white.png",
        "maxHeight":45,
        "blockClass":"twitter",
        "link": {
            "attributeNofollow": false,
            "attributeTitle": "",
            "newTab": true,
            "url": "/"
          }
    }
},
"image#test4":{
    "props":{
        "src":"https://parts.nissanusa.com/images/nissan-oem-assets/icons/social-instagram-white.png",
        "maxHeight":45,
        "blockClass":"insta",
        "link": {
            "attributeNofollow": false,
            "attributeTitle": "",
            "newTab": true,
            "url": "/"
          }
    }
},
"image#test5":{
    "props":{
        "src":"https://parts.nissanusa.com/images/nissan-oem-assets/icons/social-pinterest-white.png",
        "maxHeight":45,
        "blockClass":"pinterest",
        "link": {
            "attributeNofollow": false,
            "attributeTitle": "",
            "newTab": true,
            "url": "/"
          }
    }
},
  "accepted-payment-methods": {
    "props": {
      "showInColor": true,
      "paymentMethods": ["MasterCard", "Visa", "Diners Club"]
    }
  },
"flex-layout.row#footer-3-desktop": {
    "children": ["rich-text#disclaimer"],
     "props": {
      "blockClass": "disclaimer"
    }
  },
  "rich-text#disclaimer": {
    "props": {
      "text": "*All prices are Manufacturers Suggested Retail Prices and do not include [tax and] installation charges. Dealer sets actual price.",
      "blockClass": "disclaimer"
    }
  },
  "flex-layout.row#footer-4-desktop":{
    "children": ["rich-text#copy-right"],
     "props": {
      "blockClass": "copy-right"
    }
  },
  "rich-text#copy-right": {
    "props": {
      "text": "© 2021 Nissan North America, Inc.",
      "blockClass": "copy-right"
    }
  },
  "flex-layout.row#footer-5-desktop":{
    "children": ["rich-text#server-name"],
     "props": {
      "blockClass": "server-name"
    }
  },
  "rich-text#server-name": {
    "props": {
      "text": "SPWEB37",
      "blockClass": "server-name"
    }
  },

  "footer-layout.mobile": {
    "children": [
      "newsletter",
      "flex-layout.row#1-footer-mobile",
      "flex-layout.row#2-footer-mobile"
    ]
  },
  "flex-layout.row#2-footer-mobile": {
    "children": ["flex-layout.col#footer-1-mobile"],
    "props": {
      "blockClass": "payment-methods",
      "paddingTop": 4,
      "paddingBottom": 4
    }
  },
  "flex-layout.col#footer-1-mobile": {
    "children": [
      "accepted-payment-methods",
      "social-networks",
      "rich-text#footer-mobile"
    ],
    "props": {
      "blockClass": "payment-methods",
      "paddingTop": 4,
      "paddingBottom": 4,
      "rowGap": 4
    }
  },
  "flex-layout.row#1-footer-mobile": {
    "children": [
      "vtex.menu@2.x:menu#buying",
      "vtex.menu@2.x:menu#footer-forOwners",
      "vtex.menu@2.x:menu#footer-legal"
    ],
    "props": {
      "blockClass": "footer-link",
      "paddingTop": 6,
      "paddingBottom": 3
    }
  },
  "rich-text#footer-mobile": {
    "props": {
      "text": "© 2021 Nissan North America,Inc.",
      "blockClass": "credits"
    }
  }
}
