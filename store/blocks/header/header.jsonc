{
  "header": {
    "blocks": ["header-layout.desktop", "header-layout.mobile"]
  },
  "header.full": {
    "blocks": ["header-layout.desktop", "header-layout.mobile"]
  },
  "header-layout.desktop": {
    "children": [
      "flex-layout.row#1-desktop",
      "flex-layout.row#2-desktop",
      "flex-layout.row#4-desktop",
      // "search-bar",
      "flex-layout.row#3-desktop"
    ]
  },
  "flex-layout.row#1-desktop": {
    "children": ["notification.bar"]
  },
  // Notification-bar
  "notification.bar": {
    "props": {
      "content": "Shop the Genuine Nissan Parts and Accessories Online Catalog."
    }
  },
  //============ header row 2 =================
  "flex-layout.row#2-desktop": {
    "props": {
      "blockClass": "nissan-main-header",
      "horizontalAlign": "center",
      "verticalAlign": "center",
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "fullWidth": true
    },
    "children": [
      // "locale-switcher",
      "flex-layout.col#logo-desktop",
      // "flex-layout.col#spacer",
      "header-force-center#header-menu",
      // "flex-layout.col#header-menu",
      "flex-layout.col#spacer",
      "flex-layout.col#header-image"
      // "disclosure-layout#header-search",
      // "minicart.v2"
    ]
  },
  "flex-layout.col#logo-desktop": {
    "props": {
      "verticalAlign": "middle"
    },
    "children": ["logo#desktop"]
  },
  "flex-layout.col#header-menu": {
    "props": {
      "blockClass": "nissan-header-menu"
    },
    "children": ["rich-text#header-menu", "flex-layout.row#menu-row"]
  },
  "header-force-center#header-menu": {
    "children": ["flex-layout.col#header-menu"]
  },
  "rich-text#header-menu": {
    "props": {
      "text": "Nissan Parts & Accessories Online",
      "textPosition": "CENTER",
      "textAlignment": "CENTER",
      "blockClass": "nissan-header-text"
    }
  },
  "flex-layout.col#header-image": {
    "children": ["image#header"]
  },
  "image#header": {
    "props": {
      "src": "assets/nissan-header-img.jpg",
      "maxHeight": 50,
      "blockClass": "nissan-header-image",
      "link": {
        "attributeNofollow": false,
        "attributeTitle": "",
        "newTab": false,
        "url": "/"
      }
    }
  },
  "logo#desktop": {
    "props": {
      "title": "Logo",
      "href": "/",
      "url": "assets/nissan-logo.png",
      "blockClass": "nissan-logo",
      "width": "100"
    }
  },
  //========== "header row 2-desktop" -- End==============
  // "flex-layout.row#3-desktop" -- Start
  "flex-layout.row#3-desktop": {
    "props": {
      "blockClass": "menu-link",
      "horizontalAlign": "center",
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "fullWidth": true
    },
    "children": ["rich-text#header-menu-second"]
  },
  "rich-text#header-menu-second": {
    "props": {
      "text": "Your holiday shopping just got easier! Through 12/21, were giving you 15% off select parts and accessories* from Nissan Parts & Accessories Online! Online only. Restrictions apply.",
      "textPosition": "CENTER",
      "textAlignment": "CENTER",
      "blockClass": "nissan-header-text-second"
    }
  },
  "flex-layout.col#spacer": {
    "props": {
      "width": "grow"
    }
  },
  //=============="header row 3" -- End==================

  // "header-layout.mobile" -- Start
  "header-layout.mobile": {
    "children": [
      "flex-layout.row#1-mobile",
      "flex-layout.row#2-mobile",
      "flex-layout.row#3-mobile",
      "flex-layout.row#4-mobile",
      "flex-layout.row#5-mobile"
    ]
  },
  // "sticky-layout#1-mobile": {
  //   "children": ["flex-layout.row#1-mobile"]
  // },
  "flex-layout.row#1-mobile": {
    "children": [
      // "drawer",
      "notification.bar"
      // "logo#mobile"
      // "flex-layout.col#spacer",
      //  "overlay-trigger#search-icon",
      // "disclosure-layout#header-search",
      // "minicart.v2"
    ],
    "props": {
      "blockClass": "nissan-main-header-mobile",
      "preventHorizontalStretch": true,
      "preserveLayoutOnMobile": true,
      "fullWidth": true
    }
  },
  "flex-layout.row#2-mobile": {
    "children": ["logo#mobile", "rich-text#header-menu", "drawer"],
    "props": {
      "blockClass": "nissan-main-header-mobile",
      "preventHorizontalStretch": true,
      "preserveLayoutOnMobile": true,
      "fullWidth": true
    }
  },
  "flex-layout.row#3-mobile": {
    "children": ["search-bar"],
    "props": {
      "blockClass": "nissan-main-header-mobile",
      "preventHorizontalStretch": true,
      "preserveLayoutOnMobile": true,
      "fullWidth": true
    }
  },
  "flex-layout.row#4-mobile": {
    "children": ["overlay-trigger#bar"],
    "props": {
      "blockClass": "nissan-main-header-mobile",
      "preventHorizontalStretch": true,
      "preserveLayoutOnMobile": true,
      "fullWidth": true
    }
  },
  "flex-layout.row#5-mobile": {
    "children": ["rich-text#header-menu-second"],
    "props": {
      "blockClass": "nissan-main-header-mobile",
      "preventHorizontalStretch": true,
      "preserveLayoutOnMobile": true,
      "fullWidth": true
    }
  },

  "drawer": {
    "children": ["menu#drawer"]
  },
  "menu#drawer": {
    "children": [
      "menu-item#static-account-mobile",
      "menu-item#static-category-electronics-mobile",
      "menu-item#static-category-small-appliances-mobile",
      "menu-item#static-category-major-appliances-mobile",
      // "menu-item#static-category-test-mobile",
      "menu-item#sales"
    ],
    "props": {
      "orientation": "vertical"
    }
  },
  //----My----Account-mobile---accordion---------------------------

  "menu-item#static-account-mobile": {
    "props": {
      "id": "menu-item-account-mobile",
      "type": "custom",
      "iconId": null,
      "highlight": false,
      "blockClass": "megaMenu-header",
      "itemProps": {
        "type": "internal",
        "noFollow": true,
        "tagTitle": "Account",
        "text": "Account"
      }
    },
    "blocks": ["vtex.menu@2.x:submenu.accordion#account-mobile"]
  },
  "vtex.menu@2.x:submenu.accordion#account-mobile": {
    "props": {
      "width": "auto"
    },
    "children": ["vtex.menu@2.x:menu#account-mobile"]
  },

  "vtex.menu@2.x:menu#account-mobile": {
    "props": {
      "orientation": "vertical",
      "blockClass": "submenu-container"
    },
    "children": [
      "menu-item#account-my-account",
      "menu-item#account-my-wishlist",
      "menu-item#account-my-cart",
      "menu-item#account-checkout",
      "menu-item#account-register",
      "menu-item#account-login"
    ]
  },

  "menu-item#account-my-account": {
    "props": {
      "blockClass": "sub-header",
      "id": "menu-item-account-my-account",
      "type": "custom",
      "iconId": null,
      "highlight": false,
      "itemProps": {
        "type": "internal",
        "href": "/account",
        "noFollow": false,
        "tagTitle": "My Account",
        "text": "My Account"
