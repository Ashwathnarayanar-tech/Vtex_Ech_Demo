  "logo#mobile": {
    "props": {
      "title": "Logo",
      "href": "/",
      "url": "assets/nissan-logo.png",
      "width": "70"
    }
  },
  // "header-layout.mobile" -- End
  //----------------------main-header-menu----------------------------------------------------------
  "flex-layout.row#4-desktop": {
    "props": {
      "blockClass": "nissan-main-menu-static",
      // "horizontalAlign": "center",
      "verticalAlign": "center",
      // "preventHorizontalStretch": true,
      // "preventVerticalStretch": true,
      "fullWidth": true
    },
    "children": ["flex-layout.col#searchbar", "flex-layout.col#bar"]
  },
  "flex-layout.col#searchbar": {
    "children": ["search-bar"],
    "props": {
      "blockClass": "searchBarCol"
    }
  },
  "flex-layout.col#bar": {
    "children": ["overlay-trigger#bar"],
    "props": {
      "blockClass": "nissan-bar"
    }
  },

  "overlay-trigger#search-icon": {
    "props": {
      "blockClass": "custom-search-overlay-trigger"
    },
    "children": ["search-icon-button", "overlay-layout#search-bar"]
  },
  "overlay-layout#search-bar": {
    "children": ["search-bar"],
    "props": {
      "blockClass": "custom-search-overlay"
    }
  },
  "disclosure-layout#header-search": {
    "children": [
      "disclosure-trigger#header-trigger",
      "disclosure-content#header-content"
    ]
  },
  "disclosure-trigger#header-trigger": {
    "props": {
      "blockClass": "searchBarIcon"
    },
    "children": ["disclosure-state-indicator#header-icons"]
  },
  "disclosure-content#header-content": {
    "children": ["search-bar"]
  },
  "disclosure-state-indicator#header-icons": {
    "props": {
      "Show": "search-icon-button",
      "Hide": "remove-icon-button"
    }
  },
  "search-bar": {
    "blocks": ["autocomplete-result-list.v2"],
    "props": {
      "placeholder": "Search by Keywords,Part Number or Full VIN",
      "displayMode": "search-and-clear-buttons",
      "openAutocompleteOnFocus": true,
      "autocompleteFullWidth": true,
      "containerMode": "container",
      "blockClass": "nissan-searchbar-one",
      "hasIconLeft": true
    }
  },
  "autocomplete-result-list.v2": {
    "blocks": ["product-summary"],
    "props": {
      "data": {
        "One": "1",
        "Two": "2",
        "Three": "3"
      },
      "maxTopSearches": 5
    }
  },
  // ==================Search Vehicle Bar=============
  "overlay-trigger#bar": {
    "children": ["flex-layout.row#nissan-bar", "overlay-layout"]
  },
  "flex-layout.row#nissan-bar": {
    "children": ["rich-text#barlabel", "image#caret-bar"],
    "props": {
      "blockClass": "nissan-bar-trigger",
      "preventHorizontalStretch": true,
      "preserveLayoutOnMobile": true,
      "fullWidth": true
    }
  },
  "rich-text#barlabel": {
    "props": {
      "text": "Select Your Vehicle",
      "blockClass": "bar-label"
    }
  },
  "overlay-layout": {
    "props": {
      // "placement": "bottom"
      "showArrow": true,
      "offsets": { "skidding": 140, "distance": 0 },
      "blockClass": "bar-label-container"
    },
    "children": [
      "rich-text#barlabel2",
      "dropdown-inputbox" //React comp
    ]
  },
  "image#caret-bar": {
    "props": {
      "src": "assets/down-caret-red.png",
      "blockClass": "bar-caret-icon"
    }
  },
  "rich-text#barlabel2": {
    "props": {
      "text": "ADD VEHICLE TO GARAGE",
      "blockClass": "bar-label2"
    }
  }
}
