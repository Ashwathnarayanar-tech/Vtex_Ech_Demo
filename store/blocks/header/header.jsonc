      }
    },
    "blocks": [
      "vtex.menu@2.x:submenu.accordion#static-category-major-appliances-mobile"
    ]
  },
  "vtex.menu@2.x:submenu.accordion#static-category-major-appliances-mobile": {
    "props": {
